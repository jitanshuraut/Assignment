ASSUME CS:CODE,DS:DATA,ES:EXTRA DATA SEGMENT
STRING1 DB 'MICROPROCESSOR INTERFACING LAB$' STRING2 DB ‘AND ’
STRLEN EQU ($-STRING1) ORG 0070H
DATA ENDS EXTRA SEGMENT
ORG 0010H
STRING3 DB 38 DUP(0) EXTRA ENDS
CODE SEGMENT
START: MOV AX, DATA
MOV DS, AX MOV AX, EXTRA MOV ES, AX
MOV SI, OFFSET STRING1 MOV DI, OFFSET STRING3 CLD
MOV CX, 15 REP MOVSB CLD
MOV SI, OFFSET STRING2 MOV CX,4
REP MOVSB
MOV SI, OFFSET STRING1 ADD SI,15
MOV CX, 15 REP MOVSB
INT 3H


CODE ENDS END START