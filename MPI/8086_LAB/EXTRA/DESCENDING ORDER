ASSUME CS: CODE, DS:DATA DATA SEGMENT
LIST DW 0125H,0144H,3001H,0003H,0002H COUNT EQU 05H
DATA ENDS CODE SEGMENT
START:MOV AX,DATA
MOV DS,AX MOV DX,COUNT-1
BACK:MOV CX,DX
MOV SI,OFFSET LIST AGAIN:MOV AX,[SI]
CMP AX,[SI+2] JAE GO
XCHG AX,[SI+2] XCHG AX,[SI]
GO:INC SI
INC SI
LOOP AGAIN DEC DX
JNZ BACK INT 03H
CODE ENDS END START END