DATA SEGMENT
    DATA ENDS

CODE SEGMENT
    ASSUME CS:CODE, DS:DATA
START:MOV AX, DATA
      MOV DS,AX
      
      MOV AX,[0800H]
      ADD AX,[0700H]
      MOV [0900H],AX; 
      JC CARRY
      MOV [0902H],00 
      JMP END1
CARRY:MOV [0902H],01  

 END1: MOV AH,4CH
      INT 21H  
      CODE ENDS
END START